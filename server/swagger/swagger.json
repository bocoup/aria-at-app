{
  "swagger": "2.0",
  "info": {
    "description": "This is the API description for the ARIA-AT App server",
    "version": "1.0.0",
    "title": "ARIA-AT API"
  },
  "host": "localhost:5000",
  "basePath": "/api",
  "schemes": [
    "https",
    "http"
  ],
  "paths": {
    "/ats": {
      "get": {
        "tags": [
          "ats"
        ],
        "summary": "Get list of available AT names",
        "description": "",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "List of AT names",
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          }
        }
      }
    },
    "/runs": {
      "post": {
        "tags": [
          "runs"
        ],
        "summary": "Configure active run",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "description": "Run configuration object",
            "required": true,
            "schema": {
              "$ref": "#/definitions/RunConfiguration"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "The active run configuration",
            "schema": {
              "$ref": "#/definitions/Run"
            }
          },
          "400": {
            "description": "Error raised during configure operation"
          }
        }
      }
    },
    "/runs/active": {
      "get": {
        "tags": [
          "runs"
        ],
        "summary": "Get all active runs",
        "produces": [
          "application/json"
        ],
        "responses": {
          "201": {
            "description": "This should be changed to 200",
            "schema": {
              "$ref": "#/definitions/Run"
            }
          },
          "400": {
            "description": "Error raised during GET"
          }
        }
      }
    },
    "/runs/config": {
      "get": {
        "tags": [
          "runs"
        ],
        "summary": "Get the active configuration for the current suite of tests",
        "produces": [
          "application/json"
        ],
        "responses": {
          "201": {
            "description": "This should be changed to 200",
            "schema": {
              "$ref": "#/definitions/ActiveRunConfiguration"
            }
          }
        }
      }
    },
    "/runs/published": {
      "get": {
        "tags": [
          "runs"
        ],
        "summary": "Get all published runs",
        "produces": [
          "application/json"
        ],
        "responses": {
          "201": {
            "description": "This should be changed to 200",
            "schema": {
              "$ref": "#/definitions/Run"
            }
          },
          "400": {
            "description": "Error raised during GET"
          }
        }
      }
    },
    "/runs/status": {
      "post": {
        "tags": [
          "runs"
        ],
        "summary": "Set the status of a run",
        "produces": [
          "application/json"
        ],
        "responses": {
          "201": {
            "description": "This should change to 200",
            "schema": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "integer"
                },
                "run_status_id": {
                  "type": "integer"
                },
                "run_status": {
                  "type": "string"
                }
              }
            }
          },
          "400": {
            "description": "Error raised during POST"
          }
        }
      }
    }
  },
