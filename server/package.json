{
  "name": "server",
  "version": "1.0.0",
  "description": "Server for ARIA-AT report",
  "main": "index.js",
  "scripts": {
    "dev": "DOTENV_CONFIG_PATH=../config/dev.env nodemon --ignore './scripts/' -r dotenv/config server.js --watch",
    "dev-debug": "DEBUG=express:* DOTENV_CONFIG_PATH=../config/dev.env nodemon --inspect -r dotenv/config server.js --watch",
    "jest": "dotenv -e ../config/test.env -- jest",
    "prettier": "prettier --write \"**/*.{js,jsx,css}\"",
    "lint": "eslint --ext js,jsx .",
    "db-import-tests:dev": "DOTENV_CONFIG_PATH=../config/dev.env node -r dotenv/config ./scripts/import-tests/index.js",
    "db-import-tests:test": "DOTENV_CONFIG_PATH=../config/test.env node -r dotenv/config ./scripts/import-tests/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bocoup/aria-at-report.git"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/bocoup/aria-at-report/issues"
  },
  "homepage": "https://github.com/bocoup/aria-at-report#readme",
  "dependencies": {
    "apicache": "^1.6.2",
    "axios": "^0.19.2",
    "connect-pg-simple": "^6.2.1",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "node-html-parser": "^3.1.5",
    "nodegit": "^0.27.0",
    "nodemon": "^2.0.7",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "rawgit": "https://github.com/rgrove/rawgit.git#eaa9b36aa2143041894866ba9db72e51d05c2a52",
    "sequelize": "5.x.x"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^25.1.0",
    "moxios": "^0.4.0",
    "node-mocks-http": "^1.8.1",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2",
    "valid-url": "^1.0.9"
  }
}
